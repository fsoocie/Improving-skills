{
  "name": "improving-skills",
  "version": "1.0.0",
  "description": "app created for tracking and improving your skills",
  "main": "app.js",
  "scripts": {
    "start": "ts-node app.ts",
    "server": "nodemon app.ts",
    "client": "npm run start --prefix client",
    "build-client": "npm run build --prefix client",
    "install-client": "npm install --prefix client",
    "heroku-postbuild": "npm run install-client && npm run build-client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\" "
  },
  "keywords": [
    "React",
    "JavaScript",
    "Express",
    "Nodejs",
    "Mongoose"
  ],
  "author": "Andrey Siletskiy <fsoocie@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.36",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-google-oauth20": "^2.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "typescript": "^4.0.3"
  }
}
